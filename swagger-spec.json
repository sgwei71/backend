  "info": {
    "title": "IBK 대쉬보드 API 리스트",
    "description": "엘로드 솔루션",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "LogInRequestDto": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string",
            "description": "아이디",
            "example": "swagger12"
          },
          "password": {
            "type": "string",
            "description": "비밀번호",
            "example": "qwer1111"
          }
        },
        "required": [
          "userId",
          "password"
        ]
      },
      "ResponseDto": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "description": "응답 메시지|API 요청에 대한 응답 메시지",
            "example": "완료되었습니다."
          },
          "isSuccess": {
            "type": "boolean",
            "description": "성공여부",
            "example": true
          },
          "dataObject": {
            "type": "object",
            "description": "리턴 데이터",
            "example": {
              "test1": "test",
              "test2": "test"
            }
          },
          "dataArray": {
            "description": "리턴 데이터",
            "example": [
              {
                "test1": "test",
                "test2": "test"
              }
            ],
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "message",
          "isSuccess",
          "dataObject",
          "dataArray"
        ]
      },
      "SignUpRequestDto": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string",
            "description": "아이디",
            "example": "swagger12"
          },
          "password": {
            "type": "string",
            "description": "비밀번호",
            "example": "qwer1111"
          },
          "userType": {
            "type": "string",
            "description": "유저타입 10 || 20",
            "example": "10"
          }
        },
        "required": [
          "userId",
          "password",
          "userType"
        ]
      },
      "passwordChangeRequestDto": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string",
            "description": "아이디",
            "example": "swagger12"
          },
          "password": {
            "type": "string",
            "description": "비밀번호",
            "example": "qwer1111"
          },
          "newPassword": {
            "type": "string",
            "description": "비밀번호",
            "example": "qwer1234"
          }
        },
        "required": [
          "userId",
          "password",
          "newPassword"
        ]
      },
      "DeleteStagesDto": {
        "type": "object",
        "properties": {}
      },
      "DashboardOverallStatusDto": {
        "type": "object",
        "properties": {
          "round": {
            "type": "number",
            "description": "차수",
            "example": 1
          },
          "overallStatus": {
            "type": "string",
            "description": "전체 진행 상태",
            "example": "정상"
          }
        },
        "required": [
          "round",
          "overallStatus"
        ]
      },
      "DashboardDateDto": {
        "type": "object",
        "properties": {
          "round": {
            "type": "number",
            "description": "차수",
            "example": 1
          },
          "actualStartDateTime": {
            "type": "string",
            "description": "실제 시작일시",
            "example": "2024-10-03 07:00:00"
          },
          "estimatedEndDateTime": {
            "type": "string",
            "description": "예상 종료일시",
            "example": "2024-10-08 07:00:00"
          }
        },
        "required": [
          "round",
          "actualStartDateTime",
          "estimatedEndDateTime"
        ]
      },
      "IssueDto": {
        "type": "object",
        "properties": {
          "round": {
            "type": "number",
            "description": "차수",
            "example": 1
          },
          "issueNumber": {
            "type": "number",
            "description": "이슈사항번호",
            "example": 1
          },
          "issueContent": {
            "type": "string",
            "description": "이슈내용",
            "example": "이슈있습니다"
          }
        },
        "required": [
          "round",
          "issueNumber",
          "issueContent"
        ]
      },
      "NoticeDto": {
        "type": "object",
        "properties": {
          "round": {
            "type": "number",
            "description": "차수",
            "example": 1
          },
          "sequence": {
            "type": "number",
            "description": "공지사항번호",
            "example": 1
          },
          "noticeContent": {
            "type": "string",
            "description": "공지내용",
            "example": "1"
          },
          "lastModifiedAt": {
            "type": "string",
            "description": "최종수정일시",
            "example": "2024-10-03 07:00:00"
          },
          "lastModifier": {
            "type": "string",
            "description": "최종수정자",
            "example": "어드민"
          }
        },
        "required": [
          "round",
          "sequence",
          "noticeContent",
          "lastModifiedAt",
          "lastModifier"
        ]
      },
      "UpdateTasksDto": {
        "type": "object",
        "properties": {}
      }
    }
  }
